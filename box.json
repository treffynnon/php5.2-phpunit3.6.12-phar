{
  "compression": "BZ2",
  "algorithm": "SHA512",
  "stub": true,
  "web": false,

  "base-path": "vendor",
  "finder": [
    {
      "name": "*.php",
      "exclude": [
        "Tests",
        "Test",
        "tests",
        "test",
        "xrstf",
        "bamarni"
      ],
      "in": "."
    }
  ],
  "main": "bin/phpunit-php52",

  "git-version": "package_version",
  "output": "php52-phpunit.phar",
  "chmod": "0755"
}